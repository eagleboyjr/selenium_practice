    <test name ="Safari Grid Testing - MacOS X">
        <parameters>
            <parameter name="platform" value="MacOS" />
            <parameter name="browser"  value="safari" />
            <parameter name="version"  value="10.0.1" />
            <parameter name="url"  value="http://www.expedia.com" />
        </parameters>
        <classes>
            <class name="seleniumgridtesting.SeleniumGrid_ParallelTestFramework"></class>
        </classes>
    </test>

</suite>
